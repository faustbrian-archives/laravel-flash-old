{
    "name": "kodekeep/laravel-flash",
    "description": ":package_description",
    "keywords": [
        "kodekeep",
        "laravel-flash"
    ],
    "homepage": "https://github.com/kodekeep/laravel-flash",
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "brian@kodekeep.com",
            "homepage": "https://kodekeep.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/session": "^6.0",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "graham-campbell/analyzer": "^2.4",
        "orchestra/testbench": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "KodeKeep\\Flash\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KodeKeep\\Flash\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test:coverage": "vendor/bin/phpunit --coverage-html=.coverage --coverage-clover=coverage.xml",
        "format": "vendor/bin/php-cs-fixer fix ./"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "KodeKeep\\Flash\\FlashServiceProvider"
            ],
            "aliases": {
                "Flash": "KodeKeep\\Flash\\Facades\\Flash"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
